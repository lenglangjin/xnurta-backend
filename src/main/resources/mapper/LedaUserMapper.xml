<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leda.mapper.LedaUserMapper">

    <resultMap type="LedaUser" id="LedaUserResult">
        <result property="id"  column="id"/>

    </resultMap>

    <select id="selectByOpenid" resultType="LedaUser" parameterType="Long">

        select * from leda_user
        <where>
            openid = #{openid}
        </where>
    </select>

</mapper>